{
  "name": "react-resizable-rotatable-draggable-2",
  "version": "0.4.0",
  "description": "A rectangle react component which can be resized and rotated",
  "author": "MockingBot",
  "license": "MIT",
  "type": "module",
  "repository": "github:mockingbot/react-resizable-rotatable-draggable",
  "keywords": [
    "react",
    "rectangle",
    "resize",
    "rotate"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "scripts": {
    "build": "rollup -c",
    "build-dev": "rollup -c -w",
    "build-example": "cd example && npm install && npm run build",
    "publish-dev": "npm publish --tag dev",
    "start": "npm run build-dev",
    "deploy": "npm run build && npm run build-example && gh-pages -d example/dist",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.6.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.18.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "styled-components": "^6.1.11",
    "typescript": "^5.5.3"
  },
  "files": [
    "dist"
  ]
}
